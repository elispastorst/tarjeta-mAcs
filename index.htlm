        (function($){
            var settings = {
                // Parallax
                parallax: true,
                parallaxFactor: 0.5,
                // Skel
                skel: null,
                // Dropdown
                dropotron: true
            };
            var $window = $(window);
            $window.on('load', function(){
                // Parallax
                if (settings.parallax) {
                    $window.on('scroll.parallax', function(){
                        var scrollTop = $window.scrollTop();
                        $('body').css('background-position', 'center ' + (scrollTop * settings.parallaxFactor) + 'px');
                    });
                }
            });
            // Dropotron
            if (settings.dropotron) {
                $('#nav > ul').dropotron({
                    mode: 'fade',
                    hideDelay: 200,
                    hoverDelay: 0
                });
            }
        })(jQuery);

        /*
            main.js
        */
        (function($) {
            var $window = $(window),
                $body = $('body');

            // Breakpoints.
            breakpoints({
                xlarge:   [ '1281px',  '1680px' ],
                large:    [ '981px',   '1280px' ],
                medium:   [ '737px',   '980px'  ],
                small:    [ '481px',   '736px'  ],
                xsmall:   [ null,      '480px'  ]
            });

            // Play initial animations on page load.
            $window.on('load', function() {
                window.setTimeout(function() {
                    $body.removeClass('is-preload');
                }, 100);
            });

            // Nav.
            var $nav = $('#nav');

            if ($nav.length > 0) {

                // Dropdowns.
                $nav.find('li').has('ul').prepend('<span class="opener"></span>');

                // Toggle.
                var $nav_toggle = $('<a href="#nav" class="nav-toggle"></a>');

                $nav_toggle
                    .on('click', function() {

                        var $this = $(this);

                        $this.toggleClass('active');
                        $nav.toggleClass('visible');

                    });

                $body.append($nav_toggle);

            }
        })(jQuery);
    </script>
